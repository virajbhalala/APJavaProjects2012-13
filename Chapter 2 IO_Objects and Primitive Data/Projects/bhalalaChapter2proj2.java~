public class bhalalaChapter2proj2 {
  
  public static void main (String args[]){
    double momentum, kinetic_energy;
    EasyReader e = new EasyReader ();
    System.out.println("Give me  your mass");
    double mass=e.readDouble ();
    System.out.println("Now give me your velocity");
    double velocity=e.readDouble ();
     
     
     momentum = mass * velocity;
     
     kinetic_energy = Math.pow(velocity,2)*mass/2;
     
     System.out.println("your momentum is" + momentum);
     System.out.println("your kinetic energy is"+ kinetic_energy);
    
}
}